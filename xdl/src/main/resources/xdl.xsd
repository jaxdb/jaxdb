<xs:schema
  elementFormDefault="qualified"
  targetNamespace="http://xdb.safris.org/xdl.xsd"
  xmlns:xdl="http://xdb.safris.org/xdl.xsd"
  xmlns:xs="http://www.w3.org/2001/XMLSchema">
  
  <xs:simpleType name="name">
    <xs:restriction base="xs:NCName">
      <!-- http://dev.mysql.com/doc/refman/5.0/en/identifiers.html -->
      <xs:maxLength value="64"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="named">
    <xs:attribute name="name" type="xdl:name" use="required"/>
  </xs:complexType>
  
  <xs:complexType name="column" abstract="true">
    <xs:complexContent>
      <xs:extension base="xdl:named">
        <xs:sequence minOccurs="0">
          <xs:element name="documentation" type="xs:string" minOccurs="0"/>
          <xs:element name="foreignKey" minOccurs="0" type="xdl:foreignKey">
            <xs:annotation>
              <xs:documentation>
                There is a great potential to be able to have schema validation on the content entered into this element.
                However, the XPath implementation supported by the XMLSchema standard does not allow for the necessary complex XPath
                queries. Therefore, this element is completely unchecked by schema validation for the validity of its content.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="index" minOccurs="0" type="xdl:index"/>
        </xs:sequence>
        <xs:attribute name="null" type="xs:boolean" default="true"/>
        <xs:attribute name="checkOnUpdate">
          <xs:simpleType>
            <xs:list>
              <xs:simpleType>
                <xs:restriction base="xs:NCName">
                  <xs:enumeration value="EQUALS"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:list>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="foreignKey">
    <xs:sequence minOccurs="0">
      <xs:element name="join" type="xdl:join"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string"/>
    <xs:attribute name="references" type="xs:NCName" use="required"/>
    <xs:attribute name="column" type="xs:NCName" use="required"/>
    <xs:attribute name="onDelete" type="xdl:referentialAction" default="CASCADE"/>
    <xs:attribute name="onUpdate" type="xdl:referentialAction" default="CASCADE"/>
  </xs:complexType>

  <xs:complexType name="join">
    <xs:sequence>
      <xs:element name="field" type="xdl:field"/>
      <xs:element name="inverse" type="xdl:field" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="association" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:NCName">
          <xs:enumeration value="OneToOne"/>
          <xs:enumeration value="OneToMany"/>
          <xs:enumeration value="ManyToOne"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="fetch" type="xdl:fetch" use="required"/>
  </xs:complexType>

  <xs:complexType name="relation">
    <xs:sequence>
      <xs:element name="field" type="xdl:fieldRef"/>
      <xs:element name="inverse" type="xdl:fieldRef" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="association" type="xs:NCName" fixed="ManyToMany" use="required"/>
  </xs:complexType>

  <xs:complexType name="field">
    <xs:complexContent>
      <xs:extension base="xdl:named">
        <xs:attribute name="cascade" type="xdl:cascade" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="fieldRef">
    <xs:complexContent>
      <xs:extension base="xdl:field">
        <xs:attribute name="id" type="xs:NCName" use="required"/>
        <xs:attribute name="fetch" type="xdl:fetch" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:simpleType name="fetch">
    <xs:restriction base="xs:NCName">
      <xs:enumeration value="LAZY"/>
      <xs:enumeration value="EAGER"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="cascade">
    <xs:list>
      <xs:simpleType>
        <xs:restriction base="xs:NCName">
          <xs:enumeration value="MERGE"/>
          <xs:enumeration value="PERSIST"/>
          <xs:enumeration value="REFRESH"/>
          <xs:enumeration value="REMOVE"/>
          <xs:enumeration value="DETACH"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:list>
  </xs:simpleType>

  <xs:simpleType name="referentialAction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CASCADE"/>
      <xs:enumeration value="SET NULL"/>
      <xs:enumeration value="SET DEFAULT"/>
      <xs:enumeration value="NO ACTION"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="index">
    <xs:attribute name="unique" type="xs:boolean" use="required"/>
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="BTREE"/>
          <xs:enumeration value="HASH"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  
  <xs:complexType name="check">
    <xs:sequence>
      <xs:element name="column" type="xdl:name"/>
      <xs:element name="operator">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="="/>
            <xs:enumeration value="!="/>
            <xs:enumeration value="&gt;"/>
            <xs:enumeration value="&gt;="/>
            <xs:enumeration value="&lt;"/>
            <xs:enumeration value="&lt;="/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:choice>
        <xs:element name="column" type="xdl:name"/>
        <xs:element name="value" type="xs:string"/>
      </xs:choice>
      <xs:choice minOccurs="0">
        <xs:element name="and" type="xdl:check"/>
        <xs:element name="or" type="xdl:check"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  
  <!-- text data -->
  
  <xs:complexType name="char">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:sequence minOccurs="0">
          <xs:element name="check">
            <xs:complexType>
              <xs:attribute name="operator" use="required">
                <xs:simpleType>
                  <xs:restriction base="xs:NCName">
                    <xs:enumeration value="eq"/>
                    <xs:enumeration value="ne"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
              <xs:attribute name="condition" type="xs:string" use="required"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="length" type="xs:positiveInteger" use="required"/>
        <xs:attribute name="variant" type="xs:boolean" default="false"/>
        <xs:attribute name="default" type="xs:string"/> <!-- FIXME: Add check so the default is not longer than the length -->
        <xs:attribute name="generateOnInsert">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="UUID"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="generateOnUpdate">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="UUID"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <!-- binary data -->
  
  <xs:complexType name="bit">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:attribute name="length" type="xs:positiveInteger" use="required"/>
        <xs:attribute name="variant" type="xs:boolean" default="false"/>
        <xs:attribute name="default"> <!-- FIXME: Add check so the default is not longer than the length -->
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:pattern value="[01]+"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="blob">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:attribute name="length" type="xs:positiveInteger" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <!-- numeric data -->
  
  <xs:simpleType name="integerType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="-9223372036854775808"/>
      <xs:maxInclusive value="18446744073709551615"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="integer">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:sequence minOccurs="0">
          <xs:element name="check">
            <xs:complexType>
              <xs:attribute name="operator" use="required">
                <xs:simpleType>
                  <xs:restriction base="xs:NCName">
                    <xs:enumeration value="eq"/>
                    <xs:enumeration value="ne"/>
                    <xs:enumeration value="gt"/>
                    <xs:enumeration value="gte"/>
                    <xs:enumeration value="lt"/>
                    <xs:enumeration value="lte"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
              <xs:attribute name="condition" type="xdl:integerType" use="required"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="precision" use="required">
          <xs:simpleType>
            <xs:restriction base="xs:positiveInteger">
              <xs:minInclusive value="1"/>
              <xs:maxInclusive value="20"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="unsigned" type="xs:boolean" default="false"/>
        <xs:attribute name="default" type="xdl:integerType"/>
        <xs:attribute name="min" type="xdl:integerType"/>
        <xs:attribute name="max" type="xdl:integerType"/>
        <xs:attribute name="generateOnInsert">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="AUTO_INCREMENT"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="generateOnUpdate">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="INCREMENT"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="float">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:sequence minOccurs="0">
          <xs:element name="check">
            <xs:complexType>
              <xs:attribute name="operator" use="required">
                <xs:simpleType>
                  <xs:restriction base="xs:NCName">
                    <xs:enumeration value="eq"/>
                    <xs:enumeration value="ne"/>
                    <xs:enumeration value="gt"/>
                    <xs:enumeration value="gte"/>
                    <xs:enumeration value="lt"/>
                    <xs:enumeration value="lte"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
              <xs:attribute name="condition" type="xs:decimal" use="required"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="precision" type="xs:positiveInteger" use="required"/>
        <xs:attribute name="double" type="xs:boolean" default="false"/>
        <xs:attribute name="unsigned" type="xs:boolean" default="false"/>
        <xs:attribute name="default" type="xs:decimal"/>
        <xs:attribute name="min" type="xs:double"/>
        <xs:attribute name="max" type="xs:double"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="decimal">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:sequence minOccurs="0">
          <xs:element name="check">
            <xs:complexType>
              <xs:attribute name="operator" use="required">
                <xs:simpleType>
                  <xs:restriction base="xs:NCName">
                    <xs:enumeration value="eq"/>
                    <xs:enumeration value="ne"/>
                    <xs:enumeration value="gt"/>
                    <xs:enumeration value="gte"/>
                    <xs:enumeration value="lt"/>
                    <xs:enumeration value="lte"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
              <xs:attribute name="condition" type="xs:decimal" use="required"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="precision" type="xs:positiveInteger" use="required"/>
        <xs:attribute name="decimal" type="xs:positiveInteger" use="required"/>
        <xs:attribute name="unsigned" type="xs:boolean" default="false"/>
        <xs:attribute name="default" type="xs:decimal"/>
        <xs:attribute name="min" type="xs:decimal"/>
        <xs:attribute name="max" type="xs:decimal"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- time data -->

  <xs:complexType name="date">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:attribute name="default" type="xs:date"/>
        <xs:attribute name="generateOnInsert">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="TIMESTAMP"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="generateOnUpdate">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="TIMESTAMP"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="time">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:attribute name="precision" type="xs:nonNegativeInteger" default="0"/>
        <xs:attribute name="default" type="xs:string"/>
        <xs:attribute name="generateOnInsert">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="TIMESTAMP"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="generateOnUpdate">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="TIMESTAMP"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="dateTime">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:attribute name="precision" type="xs:nonNegativeInteger" default="0"/>
        <xs:attribute name="default" type="xs:dateTime"/>
        <xs:attribute name="generateOnInsert">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="TIMESTAMP"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="generateOnUpdate">
          <xs:simpleType>
            <xs:restriction base="xs:NCName">
              <xs:enumeration value="TIMESTAMP"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <!-- boolean -->
  
  <xs:complexType name="boolean">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:attribute name="default" type="xs:boolean"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <!-- enum -->
  
  <xs:complexType name="enum">
    <xs:complexContent>
      <xs:extension base="xdl:column">
        <xs:attribute name="values" use="required">
          <xs:simpleType>
            <xs:list>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:pattern value="[^ ]*"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:list>
          </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="default" type="xs:NMTOKEN"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="inherited">
    <xs:complexContent>
      <xs:extension base="xdl:column"/>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="table">
    <xs:complexContent>
      <xs:extension base="xdl:named">
        <xs:sequence>
          <xs:element name="column" type="xdl:column" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="constraints" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="primaryKey" minOccurs="0">
                  <xs:complexType>
                    <xs:sequence maxOccurs="unbounded">
                      <xs:element name="column" type="xdl:named"/>
                    </xs:sequence>
                  </xs:complexType>
                  <xs:unique name="primaryKeyColumn">
                    <xs:selector xpath="./xdl:column"/>
                    <xs:field xpath="@name"/>
                  </xs:unique>
                </xs:element>
                <xs:element name="foreignKey" minOccurs="0" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:complexContent>
                      <xs:extension base="xdl:foreignKey">
                        <xs:sequence>
                          <xs:element name="column" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:complexContent>
                                <xs:extension base="xdl:named">
                                  <xs:attribute name="column" type="xdl:name" use="required"/>
                                </xs:extension>
                              </xs:complexContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:extension>
                    </xs:complexContent>
                  </xs:complexType>
                </xs:element>
                <xs:element name="autoIncrement" minOccurs="0">
                  <xs:complexType>
                    <xs:attribute name="column" type="xs:NCName" use="required"/>
                  </xs:complexType>
                </xs:element>
                <xs:sequence maxOccurs="unbounded">
                  <xs:element name="unique" minOccurs="0">
                    <xs:complexType>
                      <xs:sequence maxOccurs="unbounded">
                        <xs:element name="column" type="xdl:named"/>
                      </xs:sequence>
                    </xs:complexType>
                    <xs:unique name="uniqueColumnName1">
                      <xs:selector xpath="./xdl:column"/>
                      <xs:field xpath="@name"/>
                    </xs:unique>
                  </xs:element>
                  <xs:element name="check" minOccurs="0" type="xdl:check"/>
                </xs:sequence>
              </xs:sequence>
            </xs:complexType>
            <xs:unique name="uniqueUniqueColumn">
              <xs:selector xpath="./xdl:unique"/>
              <xs:field xpath="@column"/>
            </xs:unique>
          </xs:element>
          <xs:element name="relation" type="xdl:relation" minOccurs="0"/>
          <xs:element name="triggers" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence minOccurs="1" maxOccurs="unbounded">
                <xs:element name="trigger">
                  <xs:complexType mixed="true">
                    <xs:attribute name="time" use="required">
                      <xs:simpleType>
                        <xs:restriction base="xs:token">
                          <xs:enumeration value="BEFORE"/>
                          <xs:enumeration value="AFTER"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="actions" use="required">
                      <xs:simpleType>
                        <xs:list>
                          <xs:simpleType>
                            <xs:restriction base="xs:token">
                              <xs:enumeration value="INSERT"/>
                              <xs:enumeration value="UPDATE"/>
                              <xs:enumeration value="DELETE"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:list>
                      </xs:simpleType>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="indexes" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="index">
                  <xs:complexType>
                    <xs:complexContent>
                      <xs:extension base="xdl:index">
                        <xs:sequence minOccurs="2" maxOccurs="unbounded">
                          <xs:element name="column" type="xdl:named"/>
                        </xs:sequence>
                      </xs:extension>
                    </xs:complexContent>
                  </xs:complexType>
                  <xs:unique name="indexColumn">
                    <xs:selector xpath="./xdl:column"/>
                    <xs:field xpath="@name"/>
                  </xs:unique>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="abstract" type="xs:boolean" default="false"/>
        <xs:attribute name="extends" type="xs:NCName"/>
        <xs:attribute name="skip" type="xs:boolean" default="false"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="database">
    <xs:complexContent>
      <xs:extension base="xdl:named">
        <xs:sequence>
          <xs:element name="source" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
              <xs:attribute name="xdb" type="xs:anyURI" use="required"/>
            </xs:complexType>
          </xs:element>
          <xs:element name="table" type="xdl:table" minOccurs="0" maxOccurs="unbounded">
            <!--<xs:unique name="primaryKey">
              <xs:selector xpath="*"/>
              <xs:field xpath="@primary"/>
            </xs:unique>-->
            <xs:key name="keyColumnName">
              <xs:selector xpath="./xdl:column"/>
              <xs:field xpath="@name"/>
            </xs:key>
            <xs:keyref name="keyrefColumnName1" refer="xdl:keyColumnName">
              <xs:selector xpath="./xdl:constraints/xdl:autoIncrement | ./xdl:constraints/xdl:unique/xdl:column"/>
              <xs:field xpath="@column | @name"/>
            </xs:keyref>
            <xs:keyref name="keyrefColumnName2" refer="xdl:keyColumnName">
              <xs:selector xpath="./xdl:constraints/xdl:primaryKey/xdl:column | ./xdl:constraints/xdl:foreignKey/xdl:column | ./xdl:indexes/xdl:index/xdl:column"/>
              <xs:field xpath="@name"/>
